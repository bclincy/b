<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% if title %}{{ title }}{% else %}Brian Clincy a work in Progress{% endif %}{% endblock %}</title>
    {% block css %}
        <link href="{{ asset('/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('/css/font-awesome.min.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('/css/default.css') }}">
        <meta property="og:title"
              content="{% if title %}{{ title }}{% else %}Brian Clincy the mission for greatness!{% endif %}"/>
    {% endblock %}
    {% block metadata %}
        <meta property="og:site_name" content="Brian Clincy From brianclincy.com" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta property="og:type" content="website" />
        {% if meta.canonical is defined %}
        <link rel=”canonical” href=”https://brianclincy.com/{{ meta.canonical }}”/>
        <meta property="og:url" content="https://brianclincy.com/{{ meta.canonical }}" />
        {% else %}
            <link rel=”canonical” href=”{{ base_url() }}{{ current_path(app.request.attributes.get('_route'),
                app.request.attributes.get('_route_params')) }}”/>
            <meta property="og:url" content="{{ base_url() }}{{ current_path(app.request.attributes.get('_route'),
                app.request.attributes.get('_route_params')) }}" />
        {% endif %}
        {% if keywords %}
            <meta name="keywords" content="{{ keywords }}">
        {% endif %}
        {% if meta.image %}
            <meta name="og:image" content="{{ meta.image }}">
            <meta property="og:image:secure_url" content="{{ meta.image }}" />
            <meta property="og:image:type" content="image/jpeg" />
            <meta property="og:image:width" content="{{ meta.width }}" />
            <meta property="og:image:height" content="{{ meta.height }}" />
            <meta property="og:image:alt" content="{{ meta.alt }}" />
        {% else %}
            <meta property="og:image" content="http://brianclincy.com/images/handed.jpg" />
            <meta property="og:image:secure_url" content="https://brianclincy.com/images/handed.jpg" />
            <meta property="og:image:type" content="image/jpeg" />
            <meta property="og:image:width" content="400" />
            <meta property="og:image:height" content="400" />
            <meta property="og:image:alt" content="Brian Clincy hand in his face" />
        {% endif %}
        {% if op.description %}
            <meta name="description" content="{{ description }}" />
            <meta name="og:description" content="{{ description }}" />
            {% else %}
            <meta name="description" content="brianclincy.com is the playground for all of the activities that touch
                Brian Clincy as person. It's lab of ideas put together to represent Clincyism as they come into alive.
                Check it out and see if you can catch the good vibes">
            <meta name="og:description" content="brianclincy.com is the playground for all of the activities that touch
                Brian Clincy as person. It's lab of ideas put together to represent Clincyism as they come into alive.
                Check it out and see if you can catch the good vibes" />

        {% endif %}
    {% endblock %}
</head>
<body>
<div class="loadspinner"><img src="{{ base_url() }}/images/loadingSpinner.gif" /></div>
<header class="mb-3">
    <nav class="navbar navbar-toggleable-md navbar-dark bg-faded">
        <button class="navbar-toggler navbar-toggler-right" id="navHamburger" type="button"
                data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="{{ path_for('home') }}">
            <!-- Xbox B icon by Icons8 -->
            <img class="icon icons8-Xbox-B" width="200" height="90" alt="Brian Clincy is here"
                 src="{{ asset('images/bcnobg.svg') }}">
        </a>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/about">About</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="/Community" id="communityItem" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Community
                    </a>
                    <div class="dropdown-menu" aria-labelledby="community">
                        <a class="dropdown-item" href="/community/ACHIEVE">ACHIEVE</a>
                        <a class="dropdown-item" href="/community/Love_Community_Garden">Love Community Garden</a>
                        <a class="dropdown-item" href="/community/Muskegon_area_sports">Muskegon area Sports</a>
                        <a class="dropdown-item" href="/community/Muskegon_TEDx">Muskegon TEDx</a>
                        <a class="dropdown-item" href="/community/Yuba_Love">Yuba Love</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/gallary">Gallary</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="/services" id="servicesItem" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Services
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                        <a class="dropdown-item" href="/services/Coaching">Basketball Coach</a>
                        <a class="dropdown-item" href="/services/community_engagement">Community Engagements</a>
                        <a class="dropdown-item" href="/services/Design">Graphic Designer</a>
                        <a class="dropdown-item" href="/services/Photography">Photographer</a>
                        <a class="dropdown-item" href="/services/Web">Web Development</a>
                    </div>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" type="text" placeholder="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>
    <!-- Page Heading/Breadcrumbs -->
    <div class="container">
        <div class="row" id="headerContent">
            <div class="col-lg-12">
                <h1 class="page-header">{% if title %}{{ title }}{% else %}Trying to make this work{% endif %}
                    <small>{% if subtitle %}{{ subtitle }}{% endif %}</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="{{ base_url() }}">Home</a></li>
                    {% if breadcrum %}
                        {% for b in breadcrum %}
                            <li>{{ b | raw }} </li>
                        {% endfor %}

                    {% endif %}
                </ol>
            </div>
        </div>
        <!-- /.row -->
    </div>
</header>
<div class="container" id="content">
    {% block content %}
        {% for line in data %}
            {{ dump(line) }}
        {% endfor %}
        <h1>Brian Clincy</h1>
        <p>Sweet day in the life of Zack</p>
        <div>Hello world I'm in the walk</div>
    {% endblock %}
    {% block contact %}
    {% endblock %}
</div>
{% block modal %}
    <!-- line modal -->
    <div class="modal fade" id="squarespaceModal" tabindex="-1" role="dialog" aria-labelledby="modalLabel"
         aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span
                                class="sr-only">Close</span></button>
                    <h3 class="modal-title" id="lineModalLabel">My Modal</h3>
                </div>
                <div class="modal-body">

                    <!-- content goes here -->
                    <form>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Email address</label>
                            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Password</label>
                            <input type="password" class="form-control" id="exampleInputPassword1"
                                   placeholder="Password">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputFile">File input</label>
                            <input type="file" id="exampleInputFile">
                            <p class="help-block">Example block-level help text here.</p>
                        </div>
                        <div class="checkbox">
                            <label>
                                <input type="checkbox"> Check me out
                            </label>
                        </div>
                        <button type="submit" class="btn btn-default">Submit</button>
                    </form>

                </div>
                <div class="modal-footer">
                    <div class="btn-group btn-group-justified" role="group" aria-label="group button">
                        <div class="btn-group" role="group">
                            <button type="button" class="btn btn-default" data-dismiss="modal" role="button">Close
                            </button>
                        </div>
                        <div class="btn-group btn-delete hidden" role="group">
                            <button type="button" id="delImage" class="btn btn-default btn-hover-red"
                                    data-dismiss="modal" role="button">Delete
                            </button>
                        </div>
                        <div class="btn-group" role="group">
                            <button type="button" id="saveImage" class="btn btn-default btn-hover-green"
                                    data-action="save" role="button">Save
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% include 'partials/footer.html.twig' %}
{% block javascript %}
    <script src="{{ asset('/js/tether.min.js') }}"></script>
    <script src="{{ asset('/js/jquery.min.js') }}"></script>
    <script src="{{ asset('/js/bootstrap.min.js') }}"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-126221623-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-126221623-1');
    </script>

{% endblock %}
</body>
</html>
